  "turns": [
    {
      "role": "AI",
      "en": "Hello. Where are you flying today?",
      "zh": "你好，今天飛哪裡？"
    },
    {
      "role": "USER",
      "hint_en": "To Tokyo.",
      "hint_zh": "去東京。"
    },
    {
      "role": "AI",
      "en": "May I see your passport?",
      "zh": "可以看護照嗎？"
    },
    {
      "role": "USER",
      "hint_en": "Sure. Here it is.",
      "hint_zh": "可以，給你。"
    },
    {
      "role": "AI",
      "en": "Do you have any bags to check?",
      "zh": "有行李要托運嗎？"
    },
    {
      "role": "USER",
      "hint_en": "Yes, one bag.",
      "hint_zh": "有，一件。"
    },
    {
      "role": "AI",
      "en": "How many bags?",
      "zh": "幾件？"
    },
    {
      "role": "USER",
      "hint_en": "One.",
      "hint_zh": "一件。"
    },
    {
      "role": "AI",
      "en": "Please place it on the scale.",
      "zh": "請放上秤。"
    },
    {
      "role": "USER",
      "hint_en": "Okay.",
      "hint_zh": "好。"
    },
    {
      "role": "AI",
      "en": "Thank you.",
      "zh": "謝謝。"
    },
    {
      "role": "USER",
      "hint_en": "Thanks.",
      "hint_zh": "謝謝。"
    },
    {
      "role": "AI",
      "en": "Here’s your boarding pass.",
      "zh": "這是你的登機證。"
    },
    {
      "role": "USER",
      "hint_en": "Thank you.",
      "hint_zh": "謝謝。"
    },
    {
      "role": "AI",
      "en": "Have a nice flight.",
      "zh": "祝你旅途愉快。"
    },
    {
      "role": "USER",
      "hint_en": "Thank you. Bye.",
      "hint_zh": "謝謝，再見。"
    }
  ]
