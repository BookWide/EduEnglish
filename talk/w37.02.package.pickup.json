    {
      "role": "assistant",
      "text": "What’s the name on the label?"
    },
    {
      "role": "user",
      "text": "Ray Lin."
    },
    {
      "role": "assistant",
      "text": "Okay. Was it delivered today?"
    },
    {
      "role": "user",
      "text": "Yes, this morning."
    },
    {
      "role": "assistant",
      "text": "Got it."
    },
    {
      "role": "user",
      "text": "Is it here?"
    },
    {
