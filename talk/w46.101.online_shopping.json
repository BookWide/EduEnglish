{
  "id": "w46.101.online_shopping",
  "meta": {
    "title": "W46-1 · Online Shopping（商品不符）",
    "week": 46,
    "lesson": 1
  },
  "dialogue": [
    {
      "role": "assistant",
      "text": "Hi, I received my order."
    },
    {
      "role": "user",
      "text": "Okay, what’s wrong?"
    },
    {
      "role": "assistant",
      "text": "It doesn’t match the description."
    },
    {
      "role": "user",
      "text": "How so?"
    },
    {
      "role": "assistant",
      "text": "The color is different."
    },
    {
      "role": "user",
      "text": "I’m sorry about that."
    },
    {
      "role": "assistant",
      "text": "Can I return it?"
    },
    {
      "role": "user",
      "text": "Yes, within 7 days."
    },
    {
      "role": "assistant",
      "text": "Do I pay shipping?"
    },
    {
      "role": "user",
      "text": "No, we’ll cover it."
    },
    {
      "role": "assistant",
      "text": "Great. How do I start?"
    },
    {
      "role": "user",
      "text": "I’ll email a return label."
    },
    {
      "role": "assistant",
      "text": "When will I get a refund?"
    },
    {
      "role": "user",
      "text": "After we receive it."
    },
    {
      "role": "assistant",
      "text": "Alright, thanks."
    },
    {
      "role": "user",
      "text": "You’re welcome."
    }
  ]
}