    "title": "W32-1 · Mobile Plan & Data (Find a plan)",
    "week": 32,
    "lesson": 1
  },
  "dialogue": [
    {
      "role": "assistant",
      "text": "Hi! What kind of mobile plan are you looking for?"
    },
    {
      "role": "user",
      "text": "I need a plan with data."
    },
    {
      "role": "assistant",
      "text": "How much data do you use each month?"
    },
    {
      "role": "user",
      "text": "Around ten gigabytes."
    },
    {
      "role": "assistant",
      "text": "We have a plan for that."
    },
    {
      "role": "user",
      "text": "How much is it?"
    },
    {
      "role": "assistant",
      "text": "Would you like to sign up?"
    },
    {
      "role": "user",
      "text": "Okay."
    },
    {
      "role": "assistant",
      "text": "Do you want data only or talk and data?"
    },
    {
      "role": "user",
      "text": "Talk and data, please."
    },
    {
      "role": "assistant",
      "text": "How many gigabytes do you need?"
    },
    {
      "role": "user",
      "text": "About ten gigabytes."
    },
    {
      "role": "assistant",
      "text": "Do you need unlimited calls?"
    },
    {
      "role": "user",
      "text": "Yes, unlimited calls."
    },
    {
      "role": "assistant",
      "text": "Do you want a monthly plan?"
    },
    {
      "role": "user",
      "text": "Yes, monthly."
    },
    {
      "role": "assistant",
      "text": "Any international calls?"
    },
    {
      "role": "user",
      "text": "No, just local."
    },
    {
      "role": "assistant",
      "text": "This plan is thirty dollars a month."
    },
    {
      "role": "user",
      "text": "Okay, I’ll take it."
    },
    {
      "role": "assistant",
      "text": "Would you like to sign up today?"
    },
    {
      "role": "user",
      "text": "Yes, please."
    },
    {
      "role": "assistant",
      "text": "Great. Let’s get you set up."
    },
    {
      "role": "user",
      "text": "Thank you."
    },
