  "id": "w47.102.scheduling",
  "meta": {
    "title": "W47-2 · Scheduling（衝突說明）",
    "week": 47,
    "lesson": 2
  },
  "dialogue": [
    {
      "role": "assistant",
      "text": "I have a schedule conflict."
    },
    {
      "role": "user",
      "text": "Okay, with what?"
    },
    {
      "role": "assistant",
      "text": "Another meeting moved to the same time."
    },
    {
      "role": "user",
      "text": "I see."
    },
    {
      "role": "assistant",
      "text": "Can we adjust our time?"
    },
    {
      "role": "user",
      "text": "Sure."
    },
    {
      "role": "assistant",
      "text": "What times are you free?"
    },
    {
      "role": "user",
      "text": "After 4 pm."
    },
    {
      "role": "assistant",
      "text": "How about 4:30?"
    },
    {
      "role": "user",
      "text": "That works."
    },
    {
      "role": "assistant",
      "text": "Same agenda?"
    },
    {
      "role": "user",
      "text": "Yes."
    },
    {
      "role": "assistant",
      "text": "I’ll update the calendar invite."
    },
    {
      "role": "user",
      "text": "Thanks."
    },
    {
      "role": "assistant",
      "text": "Talk later."
    },
    {
      "role": "user",
      "text": "See you."
    }
  ]
}