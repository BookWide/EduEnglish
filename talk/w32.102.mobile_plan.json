  "meta": {
    "title": "W32-2 · Mobile Plan & Data (Contract length)",
    "week": 32,
    "lesson": 2
  },
  "dialogue": [
    {
      "role": "assistant",
      "text": "Do you prefer a monthly plan or a one‑year contract?"
    },
    {
      "role": "user",
      "text": "What’s the difference?"
    },
    {
      "role": "assistant",
      "text": "The contract is cheaper, but it locks you in."
    },
    {
      "role": "user",
      "text": "How much cheaper?"
    },
    {
      "role": "assistant",
      "text": "Five dollars less per month."
    },
    {
      "role": "user",
      "text": "Okay, I’ll do monthly."
    },
    {
      "role": "assistant",
      "text": "Let’s continue."
    },
    {
      "role": "user",
      "text": "Okay."
    },
    {
      "role": "assistant",
      "text": "Do you want data only or talk and data?"
    },
    {
      "role": "user",
      "text": "Talk and data, please."
    },
    {
      "role": "assistant",
      "text": "How many gigabytes do you need?"
    },
    {
      "role": "user",
      "text": "About ten gigabytes."
    },
    {
      "role": "assistant",
      "text": "Do you need unlimited calls?"
    },
    {
      "role": "user",
      "text": "Yes, unlimited calls."
    },
    {
      "role": "assistant",
      "text": "Do you want a monthly plan?"
    },
    {
      "role": "user",
      "text": "Yes, monthly."
    },
    {
      "role": "assistant",
      "text": "Any international calls?"
    },
    {
      "role": "user",
      "text": "No, just local."
    },
    {
      "role": "assistant",
      "text": "This plan is thirty dollars a month."
    },
    {
      "role": "user",
      "text": "Okay, I’ll take it."
    },
    {
      "role": "assistant",
      "text": "Would you like to sign up today?"
    },
    {
      "role": "user",
      "text": "Yes, please."
    },
    {
      "role": "assistant",
      "text": "Great. Let’s get you set up."
    },
    {
      "role": "user",
      "text": "Thank you."
    },
