  "dialogue": [
    {
      "role": "assistant",
      "text": "Hey, can we talk?"
    },
    {
      "role": "user",
      "text": "Sure."
    },
    {
      "role": "assistant",
      "text": "I need to cancel tonight."
    },
    {
      "role": "user",
      "text": "Oh, what happened?"
    },
    {
      "role": "assistant",
      "text": "Something came up at work."
    },
    {
      "role": "user",
      "text": "I understand."
    },
    {
      "role": "assistant",
      "text": "I’m really sorry."
    },
    {
      "role": "user",
      "text": "It’s okay."
    },
    {
      "role": "assistant",
      "text": "Can we do another day?"
    },
    {
      "role": "user",
      "text": "Yes, how about Saturday?"
    },
    {
      "role": "assistant",
      "text": "Saturday works."
    },
    {
      "role": "user",
      "text": "Great."
    },
    {
      "role": "assistant",
      "text": "Same time?"
    },
    {
      "role": "user",
      "text": "Yes, 7 pm."
    },
    {
      "role": "assistant",
      "text": "Perfect. See you then."
    },
    {
      "role": "user",
      "text": "See you."
    },
    {
      "role": "assistant",
      "text": "Can you help me, please?"
    },
    {
      "role": "user",
      "text": "Yes, sure."
    },
    {
      "role": "assistant",
      "text": "What do you need?"
    },
    {
      "role": "user",
      "text": "I need some information."
    },
    {
      "role": "assistant",
      "text": "Could you say that again?"
    },
    {
      "role": "user",
      "text": "Sure, I need help with this."
    },
    {
      "role": "assistant",
      "text": "When do you need it?"
    },
    {
      "role": "user",
      "text": "Today, if possible."
    },
    {
      "role": "assistant",
      "text": "Is this okay?"
    },
    {
      "role": "user",
      "text": "Yes, that’s fine."
    },
    {
      "role": "assistant",
      "text": "Anything else?"
    },
    {
      "role": "user",
      "text": "No, that’s all."
    },
    {
      "role": "assistant",
      "text": "Thank you."
    },
    {
      "role": "user",
      "text": "You’re welcome."
    },
    {
      "role": "assistant",
      "text": "Have a nice day."
    },
    {
      "role": "user",
      "text": "You too."
    },
