  "turns": [
    {
      "role": "assistant",
      "text": "Hello. Where are you flying today?"
    },
    {
      "role": "user",
      "text": "Okay."
    },
    {
      "role": "assistant",
      "text": "May I see your passport?"
    },
    {
      "role": "user",
      "text": "Okay."
    },
    {
      "role": "assistant",
      "text": "Do you have any bags to check?"
    },
    {
      "role": "user",
      "text": "Okay."
    },
    {
      "role": "assistant",
      "text": "How many bags?"
    },
    {
      "role": "user",
      "text": "Okay."
    },
    {
      "role": "assistant",
      "text": "Please place it on the scale."
    },
    {
      "role": "user",
      "text": "Okay."
    },
    {
      "role": "assistant",
      "text": "Thank you."
    },
    {
      "role": "user",
      "text": "Okay."
    },
    {
      "role": "assistant",
      "text": "Here’s your boarding pass."
    },
    {
      "role": "user",
      "text": "Okay."
    },
    {
      "role": "assistant",
      "text": "Have a nice flight."
    },
    {
      "role": "user",
      "text": "Yes, please."
    },
    {
      "role": "assistant",
      "text": "Anything else you need?"
    },
    {
      "role": "user",
      "text": "Yes."
    },
    {
      "role": "assistant",
      "text": "Do you have a preference?"
    },
    {
      "role": "user",
      "text": "No, that's fine."
    },
    {
      "role": "assistant",
      "text": "Could you confirm that for me?"
    },
    {
      "role": "user",
      "text": "I prefer something simple."
    },
    {
      "role": "assistant",
      "text": "Would you like an option with a better price?"
    },
    {
      "role": "user",
      "text": "That works."
    },
    {
      "role": "assistant",
      "text": "Is this for today or later?"
    },
    {
      "role": "user",
      "text": "Today."
    },
    {
      "role": "assistant",
      "text": "Do you want to add anything?"
    },
    {
      "role": "user",
      "text": "No, thanks."
    },
    {
      "role": "assistant",
      "text": "Let me double-check that."
    },
    {
      "role": "user",
      "text": "Okay."
    }
