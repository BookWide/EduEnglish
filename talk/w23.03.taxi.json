  "turns": [
    {
      "role": "teacher",
      "text": "Can you help me, please?"
    },
    {
      "role": "student",
      "text": "Yes, sure."
    },
    {
      "role": "teacher",
      "text": "What do you need?"
    },
    {
      "role": "student",
      "text": "I need some information."
    },
    {
      "role": "teacher",
      "text": "Could you say that again?"
    },
    {
      "role": "student",
      "text": "Sure, I need help with this."
    },
    {
      "role": "teacher",
      "text": "When do you need it?"
    },
    {
      "role": "student",
      "text": "Today, if possible."
    },
    {
      "role": "teacher",
      "text": "Is this okay?"
    },
    {
      "role": "student",
      "text": "Yes, that’s fine."
    },
    {
      "role": "teacher",
      "text": "Anything else?"
    },
    {
      "role": "student",
      "text": "No, that’s all."
    },
    {
      "role": "teacher",
      "text": "Thank you."
    },
    {
      "role": "student",
      "text": "You’re welcome."
    },
    {
      "role": "teacher",
      "text": "Have a nice day."
    },
    {
      "role": "student",
      "text": "You too."
