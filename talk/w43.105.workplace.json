  "meta": {
    "title": "W43-5 · Workplace（婉拒加工作）",
    "week": 43,
    "lesson": 5
  },
  "dialogue": [
    {
      "role": "assistant",
      "text": "Can you take another request?"
    },
    {
      "role": "user",
      "text": "I’d like to, but I can’t today."
    },
    {
      "role": "assistant",
      "text": "Are you overloaded?"
    },
    {
      "role": "user",
      "text": "Yes, I’m at capacity."
    },
    {
      "role": "assistant",
      "text": "When can you do it?"
    },
    {
      "role": "user",
      "text": "Next week."
    },
    {
      "role": "assistant",
      "text": "We need it sooner."
    },
    {
      "role": "user",
      "text": "Could someone else cover it?"
    },
    {
      "role": "assistant",
      "text": "I’ll ask the team."
    },
    {
      "role": "user",
      "text": "Thank you."
    },
    {
      "role": "assistant",
      "text": "I don’t want to rush and risk mistakes."
    },
    {
      "role": "user",
      "text": "That’s fair."
    },
    {
      "role": "assistant",
      "text": "Let’s revisit tomorrow."
    },
    {
      "role": "user",
      "text": "Okay."
    },
    {
      "role": "assistant",
      "text": "Thanks for understanding."
    },
    {
      "role": "user",
      "text": "Of course."
    }
  ]
}