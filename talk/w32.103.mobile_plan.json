  "dialogue": [
    {
      "role": "assistant",
      "text": "Do you need a physical SIM or an eSIM?"
    },
    {
      "role": "user",
      "text": "What’s an eSIM?"
    },
    {
      "role": "assistant",
      "text": "It’s a digital SIM inside your phone."
    },
    {
      "role": "user",
      "text": "Can my phone use it?"
    },
    {
      "role": "assistant",
      "text": "What model do you have?"
    },
    {
      "role": "user",
      "text": "An iPhone 13."
    },
    {
      "role": "assistant",
      "text": "We can activate it today."
    },
    {
      "role": "user",
      "text": "Okay."
    },
    {
      "role": "assistant",
      "text": "Do you prefer a contract or no contract?"
    },
    {
      "role": "user",
      "text": "No contract, please."
    },
    {
      "role": "assistant",
      "text": "Do you need a new SIM card?"
    },
    {
      "role": "user",
      "text": "Yes, a new SIM card."
    },
    {
      "role": "assistant",
      "text": "Do you want a physical SIM or eSIM?"
    },
    {
      "role": "user",
      "text": "eSIM, please."
    },
    {
      "role": "assistant",
      "text": "Do you travel often?"
    },
    {
      "role": "user",
      "text": "Sometimes, maybe once a month."
    },
    {
      "role": "assistant",
      "text": "We have a roaming add-on."
    },
    {
      "role": "user",
      "text": "How much is the roaming add-on?"
    },
    {
      "role": "assistant",
      "text": "It’s five dollars per month."
    },
    {
      "role": "user",
      "text": "Okay, add it."
    },
    {
      "role": "assistant",
      "text": "Can you confirm your ID and address?"
    },
    {
      "role": "user",
      "text": "Sure, here you go."
    },
    {
      "role": "assistant",
      "text": "You’ll be active in a few minutes."
    },
