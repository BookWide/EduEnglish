  "dialogue": [
    {
      "role": "assistant",
      "text": "Hi, support team."
    },
    {
      "role": "user",
      "text": "Hi, I can’t log in."
    },
    {
      "role": "assistant",
      "text": "What error do you see?"
    },
    {
      "role": "user",
      "text": "It says my account is locked."
    },
    {
      "role": "assistant",
      "text": "When did this start?"
    },
    {
      "role": "user",
      "text": "This morning."
    },
    {
      "role": "assistant",
      "text": "Any password changes recently?"
    },
    {
      "role": "user",
      "text": "No."
    },
    {
      "role": "assistant",
      "text": "I see too many attempts. I can unlock it."
    },
    {
      "role": "user",
      "text": "Please do."
    },
    {
      "role": "assistant",
      "text": "Done. Try logging in now."
    },
    {
      "role": "user",
      "text": "Okay… it works."
    },
    {
      "role": "assistant",
      "text": "Would you like to reset your password?"
    },
    {
      "role": "user",
      "text": "Yes, for safety."
    },
    {
      "role": "assistant",
      "text": "I’ll send a reset link."
    },
    {
      "role": "user",
      "text": "Thanks."
    },
    {
      "role": "assistant",
      "text": "Can you help me, please?"
    },
    {
      "role": "user",
      "text": "Yes, sure."
    },
    {
      "role": "assistant",
      "text": "What do you need?"
    },
    {
      "role": "user",
      "text": "I need some information."
    },
    {
      "role": "assistant",
      "text": "Could you say that again?"
    },
    {
      "role": "user",
      "text": "Sure, I need help with this."
    },
    {
      "role": "assistant",
      "text": "When do you need it?"
    },
    {
      "role": "user",
      "text": "Today, if possible."
    },
    {
      "role": "assistant",
      "text": "Is this okay?"
    },
    {
      "role": "user",
      "text": "Yes, that’s fine."
    },
    {
      "role": "assistant",
      "text": "Anything else?"
    },
    {
      "role": "user",
      "text": "No, that’s all."
    },
    {
      "role": "assistant",
      "text": "Thank you."
    },
    {
      "role": "user",
      "text": "You’re welcome."
    },
    {
      "role": "assistant",
      "text": "Have a nice day."
    },
    {
      "role": "user",
      "text": "You too."
    },
