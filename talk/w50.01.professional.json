  "dialogue": [
    {
      "role": "assistant",
      "text": "Thanks for your time today."
    },
    {
      "role": "user",
      "text": "Of course."
    },
    {
      "role": "assistant",
      "text": "I’d like to discuss the next steps."
    },
    {
      "role": "user",
      "text": "Sure."
    },
    {
      "role": "assistant",
      "text": "First, here’s the goal."
    },
    {
      "role": "user",
      "text": "Okay."
    },
    {
      "role": "assistant",
      "text": "We want a smooth rollout."
    },
    {
      "role": "user",
      "text": "Agreed."
    },
    {
      "role": "assistant",
      "text": "I’ll keep it brief."
    },
    {
      "role": "user",
      "text": "Go ahead."
    },
    {
      "role": "assistant",
      "text": "We have two key items."
    },
    {
      "role": "user",
      "text": "What are they?"
    },
    {
      "role": "assistant",
      "text": "Timeline and responsibilities."
    },
    {
      "role": "user",
      "text": "Alright."
    },
    {
      "role": "assistant",
      "text": "Let’s start with timeline."
    },
    {
      "role": "user",
      "text": "Okay."
    },
    {
      "role": "assistant",
      "text": "What do you do for work?"
    },
    {
      "role": "user",
      "text": "I work in business."
    },
    {
      "role": "assistant",
      "text": "How long have you worked there?"
    },
    {
      "role": "user",
      "text": "About two years."
    },
    {
      "role": "assistant",
      "text": "Do you like your job?"
    },
    {
      "role": "user",
      "text": "Yes, I do."
    },
    {
      "role": "assistant",
      "text": "Are you busy this week?"
    },
    {
      "role": "user",
      "text": "A little busy."
    },
    {
      "role": "assistant",
      "text": "Do you have any meetings today?"
    },
    {
      "role": "user",
      "text": "Yes, one meeting."
    },
    {
      "role": "assistant",
      "text": "What time is it?"
    },
    {
      "role": "user",
      "text": "At 3 p.m."
    },
    {
      "role": "assistant",
      "text": "Good luck."
    },
    {
      "role": "user",
      "text": "Thanks!"
    },
    {
      "role": "assistant",
      "text": "Talk to you later."
    },
    {
      "role": "user",
      "text": "See you."
    },
    {
