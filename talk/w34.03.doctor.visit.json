  "dialogue": [
    {
      "role": "assistant",
      "text": "Do you have health insurance?"
    },
    {
      "role": "user",
      "text": "Yes."
    },
    {
      "role": "assistant",
      "text": "Please bring your insurance card."
    },
    {
      "role": "user",
      "text": "Okay."
    },
    {
      "role": "assistant",
      "text": "Any allergies we should know about?"
    },
    {
      "role": "user",
      "text": "Penicillin."
    },
    {
      "role": "assistant",
      "text": "I’ll note that in your file."
    },
    {
      "role": "user",
      "text": "Okay."
    },
    {
      "role": "assistant",
      "text": "Could you explain the options?"
    },
    {
      "role": "user",
      "text": "Yes, we have a few options."
    },
    {
      "role": "assistant",
      "text": "Which one do you recommend?"
    },
    {
      "role": "user",
      "text": "I recommend the standard one."
    },
    {
      "role": "assistant",
      "text": "What’s the difference?"
    },
    {
      "role": "user",
      "text": "It’s faster and includes support."
    },
    {
      "role": "assistant",
      "text": "How much does it cost?"
    },
    {
      "role": "user",
      "text": "It’s thirty dollars."
    },
    {
      "role": "assistant",
      "text": "Can I change later?"
    },
    {
      "role": "user",
      "text": "Yes, you can upgrade anytime."
    },
    {
      "role": "assistant",
      "text": "How do I sign up?"
    },
    {
      "role": "user",
      "text": "Just fill out this form."
    },
    {
      "role": "assistant",
      "text": "When will it be ready?"
    },
    {
      "role": "user",
      "text": "Within a day."
    },
    {
      "role": "assistant",
      "text": "Sounds good."
    },
    {
      "role": "user",
      "text": "Great."
