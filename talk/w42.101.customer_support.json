  "id": "w42.101.customer_support",
  "meta": {
    "title": "W42-1 · Customer Support（無法登入）",
    "week": 42,
    "lesson": 1
  },
  "dialogue": [
    {
      "role": "assistant",
      "text": "Hi, support team."
    },
    {
      "role": "user",
      "text": "Hi, I can’t log in."
    },
    {
      "role": "assistant",
      "text": "What error do you see?"
    },
    {
      "role": "user",
      "text": "It says my account is locked."
    },
    {
      "role": "assistant",
      "text": "When did this start?"
    },
    {
      "role": "user",
      "text": "This morning."
    },
    {
      "role": "assistant",
      "text": "Any password changes recently?"
    },
    {
      "role": "user",
      "text": "No."
    },
    {
      "role": "assistant",
      "text": "I see too many attempts. I can unlock it."
    },
    {
      "role": "user",
      "text": "Please do."
    },
    {
      "role": "assistant",
      "text": "Done. Try logging in now."
    },
    {
      "role": "user",
      "text": "Okay… it works."
    },
    {
      "role": "assistant",
      "text": "Would you like to reset your password?"
    },
    {
      "role": "user",
      "text": "Yes, for safety."
    },
    {
      "role": "assistant",
      "text": "I’ll send a reset link."
    },
    {
      "role": "user",
      "text": "Thanks."
    }
  ]
}