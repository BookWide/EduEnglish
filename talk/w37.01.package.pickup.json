  "dialogue": [
    {
      "role": "assistant",
      "text": "Hello. How can I help you?"
    },
    {
      "role": "user",
      "text": "I’m here to pick up a package."
    },
    {
      "role": "assistant",
      "text": "Sure. What’s your apartment number?"
    },
    {
      "role": "user",
      "text": "Apt 507."
    },
    {
      "role": "assistant",
      "text": "Do you have a photo ID?"
    },
    {
      "role": "user",
      "text": "Yes."
    },
    {
      "role": "assistant",
      "text": "Let me check the storage room."
    },
    {
      "role": "user",
      "text": "Okay."
    },
    {
      "role": "assistant",
      "text": "Can you help me, please?"
    },
    {
      "role": "user",
      "text": "Yes, sure."
    },
    {
      "role": "assistant",
      "text": "What do you need?"
    },
    {
      "role": "user",
      "text": "I need some information."
    },
    {
      "role": "assistant",
      "text": "Could you say that again?"
    },
    {
      "role": "user",
      "text": "Sure, I need help with this."
    },
    {
      "role": "assistant",
      "text": "When do you need it?"
    },
    {
      "role": "user",
      "text": "Today, if possible."
    },
    {
      "role": "assistant",
      "text": "Is this okay?"
    },
    {
      "role": "user",
      "text": "Yes, that’s fine."
    },
    {
      "role": "assistant",
      "text": "Anything else?"
    },
    {
      "role": "user",
      "text": "No, that’s all."
    },
    {
      "role": "assistant",
      "text": "Thank you."
    },
    {
      "role": "user",
      "text": "You’re welcome."
    },
    {
      "role": "assistant",
      "text": "Have a nice day."
    },
    {
      "role": "user",
      "text": "You too."
