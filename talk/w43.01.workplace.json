      "text": "When do you need it?"
    },
    {
      "role": "user",
      "text": "Today, if possible."
    },
    {
      "role": "assistant",
      "text": "Is this okay?"
    },
    {
      "role": "user",
      "text": "Yes, that’s fine."
    },
    {
      "role": "assistant",
      "text": "Anything else?"
    },
    {
      "role": "user",
      "text": "No, that’s all."
